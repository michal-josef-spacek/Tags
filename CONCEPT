CONCEPT
=======
- ['b', 'element']               # "<element>"
- ['e', 'element']               # "</element>"
- ['c', 'data', 'data']          # "<!--datadata-->"
- ['d', 'data', 'data']          # "datadata"
- ['i', 'target', 'data']        # "<?target data?>
- ['a', 'par', 'val']            # 'par="val" par2="val2"'
- ['r', "text\ntext"]            # "text\ntext"
- ['cd', "cdata section <element>"]  # "<![CDATA[cdata section <element>]]>"
